#include <iostream>

using namespace std;

int i, j;
int seq[] = {2,7,9,4,1,5,3,6,0,8};
int n = (sizeof(seq)/sizeof(int));


int main(){

//bubble sort
    int x;

    for(i = 1; i <= n-1; i++){
        for(j=0; j <= n-i-1; j++){
            if(seq[j] > seq[j+1]){
                swap(seq[j], seq[j+1]);
            }

        }

    }

   //Show the reordered array on screen
   for(x = 0; x < n; x++){
    cout << seq[x]<<endl;
    }


return 0;
}
